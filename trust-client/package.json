{
    "name": "xxweb",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:client": "vue-cli-service build",
        "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build --mode server",
        "build:win": "npm run build:server && mv dist/vue-ssr-server-bundle.json bundle && npm run build:client && mv bundle dist/vue-ssr-server-bundle.json",
        "dev:serve": "cross-env WEBPACK_TARGET=node node ./server/ssr.js",
        "dev": "concurrently \"npm run serve\" \"npm run dev:serve\" ",
        "lint": "vue-cli-service lint",
        "t:client": "cross-env NODE_ENV=dev webpack --config ./build/vue.config.js --watch --progress --hide-modules",
        "t:server": "cross-env NODE_ENV=dev WEBPACK_TARGET=node webpack --config ./build/vue.config.js  --watch --progress --hide-modules",
        "test": "webpack --config build/webpack.client.dev.js"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "echarts": "^3.8.5",
        "compression": "^1.6.2",
        "es6-promise": "^4.0.5",
        "express": "^4.14.0",
        "lru-cache": "^4.0.2",
        "serialize-javascript": "^1.3.0",
        "serve-favicon": "^2.3.2"
    },
    "devDependencies": {
        "autoprefixer": "^6.6.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-1": "^6.24.1",
        "cross-env": "^3.1.3",
        "css-loader": "^0.26.1",
        "eslint": "^4.13.1",
        "eslint-friendly-formatter": "^3.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-html": "^4.0.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-prettier": "^2.5.0",
        "eslint-plugin-vue": "^4.0.0-beta.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^0.11.2",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^2.24.1",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "memory-fs": "^0.4.1",
        "node-sass": "^4.1.1",
